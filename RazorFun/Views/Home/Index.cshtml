<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Hello!</title>
    </head>
    <body>
        @{
            List<string> StringList = new List<string>() {"Apple Pie", "Pizza", "Cinnamon Rolls", "Lasagna", "Donuts", "Chocolate Cake", "Burritos"};

                <h1>Here are some delicious foods!</h1>
                @foreach(string food in StringList)
                {
                    <div>
                        <p>@food</p>
                    </div>
                }
        }
    </body>
</html>

